    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglify-js": "^3.1.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0"
  },
  "engines": {
    "node": ">=4"
